  "scripts": {
    "build": "turbo build",
    "build:force": "turbo build --force",
    "merge:md": "node ./tools/merge-md.cjs --root ./ ./.doc-bundles/",
    "docs:update": "node ./tools/update-readme.cjs",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "typecheck": "turbo typecheck",
    "changeset": "changeset",
    "version-packages": "changeset version",
